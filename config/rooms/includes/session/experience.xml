<?xml version="1.0" encoding="UTF-8"?>
<Include xmlns:xi="http://www.w3.org/2001/XInclude">
	<Experience id="experience" points="0" persist="false">
		<Level id="level_0" name="Level 1" points="0"/>
		<Level id="level_1" name="Level 2" points="1"/>
		<Level id="level_2" name="Level 3" points="2"/>
		<Level id="level_3" name="Level 4" points="3"/>
		<Level id="level_4" name="Level 5" points="4"/>
		<Level id="level_5" name="Level 6" points="5"/>
	
		<Triggers>
			<OnLevelUp>
				<Trace message="OnLevelUp level={Event.level}"/>
			</OnLevelUp>
			<OnAwardedExperience>
				<Trace message="OnAwardedExperience level={Experience.level}"/>
			</OnAwardedExperience>
		</Triggers>
	</Experience>
</Include>