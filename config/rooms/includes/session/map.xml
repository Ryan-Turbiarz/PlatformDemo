<?xml version="1.0" encoding="UTF-8"?>
<Include xmlns:xi="http://www.w3.org/2001/XInclude">
	<Map>
		<Location name="Professor's Lab" roomID="lab" id="labRoom" visible="true" visited="false"/>		
		<Location name="Pirate Village" roomID="village" id="villageRoom" visible="true" visited="false"/>	

		<Triggers>
			<OnLocationVisible>
				<Trace message="Location visible location={Event.location}"/>
