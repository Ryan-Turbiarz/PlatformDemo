<?xml version="1.0" encoding="UTF-8"?>
<ChatRoom id="cinderella" xmlns:xi="http://www.w3.org/2001/XInclude" startX="1300" startY="800">

	<xi:include href="includes/room_config.xml"/>
		
	<CollisionMap id="walkableArea" assetID="hit_asset" />

	<Filters id="playerFilters">
		<DepthFilter id="playerDepthFilter" scaleBack="-0.4" scaleFront="0.7"/>
		<LightMapFilter assetID="light_filter_asset" />
	</Filters>

	<Scene id="scene" target="{Room.player}" viewportWidth="1024" viewportHeight="768" width="1500" height="900">
		
		<ParallaxLayer mouseEnabled="false" xm="0.1" ym="0.1">
			<DisplayObject2D assetID="sky_asset" mouseEnabled="false" x="-425" y="-120"/>
		</ParallaxLayer>
		
		<ParallaxLayer mouseEnabled="false" xm="0.9" ym="0.9">
			<DisplayObject2D assetID="parrallax_asset" />
		</ParallaxLayer>
	
		<DisplayObject2D assetID="floor_asset" mouseEnabled="false"/>
		<DisplayObject2D id="click" assetID="click_asset" mouseEnabled="true" clickThrough="true" alpha="0"/>
	 
		<ZSortingDisplayObject id="furniture">
			<DisplayObject2D id="tall_grass" assetID="tall_grass_asset" x="1231" y="({Room.tall_grass.originY}+{569})" originX="80" originY="144" />
			<DisplayObject2D id="flower_4" assetID="flower_4_asset" x="1185" y="({Room.flower_4.originY}+{650})" originX="37" originY="58" />
			<DisplayObject2D id="flower_3" assetID="flower_3_asset" x="500" y="({Room.flower_3.originY}+{614})" originX="49" originY="45" />
			<DisplayObject2D id="flower_2" assetID="flower_2_asset" x="533" y="({Room.flower_2.originY}+{650})" originX="49" originY="54" />
			<DisplayObject2D id="flower_1" assetID="flower_1_asset" x="721" y="({Room.flower_1.originY}+{712})" originX="56" originY="63" />
			<DisplayObject2D id="tree_1" assetID="tree_1_asset" x="1458" y="({Room.tree_1.originY}+{-212})" originX="182" originY="776" />
			<DisplayObject2D id="tree_2" assetID="tree_2_asset" x="1450" y="({Room.tree_2.originY}+{236})" originX="186" originY="368" />
			<DisplayObject2D id="carriage" assetID="carriage_asset" x="643" y="({Room.carriage.originY}+{400})" originX="177" originY="192" />
			<DisplayObject2D id="grass copy" assetID="grass copy_asset" x="552" y="({Room.grass copy.originY}+{640})" originX="76" originY="59" />
			<DisplayObject2D id="sign" assetID="sign_asset" x="1205" y="({Room.sign.originY}+{405})" originX="186" originY="270" />
			<DisplayObject2D id="tree_3" assetID="tree_3_asset" x="59" y="({Room.tree_3.originY}+{123})" originX="297" originY="553" />
			<DisplayObject2D id="rock" assetID="rock_asset" x="125" y="({Room.rock.originY}+{551})" originX="115" originY="104" />
			<DisplayObject2D id="stump" assetID="stump_asset" x="392" y="({Room.stump.originY}+{589})" originX="108" originY="193" />
			<DisplayObject2D id="handle_2" assetID="handle_2_asset" x="764" y="({Room.handle_2.originY}+{518})" originX="46" originY="118" />
			<DisplayObject2D id="handle_1" assetID="handle_1_asset" x="578" y="({Room.handle_1.originY}+{535})" originX="31" originY="140" />
		</ZSortingDisplayObject>
		
		<DisplayObject2D id="coll" assetID="coll_1_asset" x="1440" y="823" collidable="true" distance="170" alpha="0"/>
		
		<ParallaxLayer mouseEnabled="false" xm="1.2" ym="1">
			<DisplayObject2D id="foreground_2" assetID="foreground_2_asset" x="0" y="0" />
			<DisplayObject2D id="foreground" assetID="foreground_asset" x="0" y="568" />
		</ParallaxLayer>
		
		<ZSortingDisplayObject id="playerMessages"/>
	</Scene>
	
	<Assets>
      	<Asset id="handle_1_asset" url="images/cinderella/handle_1.png" />
      	<Asset id="handle_2_asset" url="images/cinderella/handle_2.png" />
      	<Asset id="sky_asset" url="images/cinderella/sky.jpg" />
		<Asset id="parrallax_asset" url="images/cinderella/mid.png" />
		<Asset id="floor_asset" url="images/cinderella/background.png" />
		<Asset id="tall_grass_asset" url="images/cinderella/tall_grass.png" />
		<Asset id="flower_4_asset" url="images/cinderella/flower_4.png" />
		<Asset id="flower_3_asset" url="images/cinderella/flower_3.png" />
		<Asset id="flower_2_asset" url="images/cinderella/flower_2.png" />
		<Asset id="flower_1_asset" url="images/cinderella/flower_1.png" />
		<Asset id="tree_1_asset" url="images/cinderella/tree_1.png" />
		<Asset id="tree_2_asset" url="images/cinderella/tree_2.png" />
		<Asset id="carriage_asset" url="images/cinderella/carriage.png" />
		<Asset id="grass copy_asset" url="images/cinderella/grass copy.png" />
		<Asset id="sign_asset" url="images/cinderella/sign.png" />
		<Asset id="tree_3_asset" url="images/cinderella/tree_3.png" />
		<Asset id="rock_asset" url="images/cinderella/rock.png" />
		<Asset id="stump_asset" url="images/cinderella/stump.png" />
		<Asset id="foreground_2_asset" url="images/cinderella/foreground_2.png" />
		<Asset id="foreground_asset" url="images/cinderella/foreground.png" />
		<Asset id="click_asset" url="images/cinderella/click.gif" />
		<Asset id="hit_asset" url="images/cinderella/hit.gif" />
		<Asset id="coll_1_asset" url="images/beanstalk/coll_1.png" />
	</Assets>	
	
	<Triggers>
		<onFixup>
		</onFixup>
	</Triggers>
	
	<Trigger gameObject="{Room.click}" event="onClick">
		<MoveTo gameObject="{Room.player}" targetX="{Event.x}" targetY="{Event.y}" />
	</Trigger>
	
	<Trigger gameObject="{Room.coll}" event="onEnterCollision">
		<ChangeRoom roomID="beanstalk" />
	</Trigger>
 
	
</ChatRoom>	