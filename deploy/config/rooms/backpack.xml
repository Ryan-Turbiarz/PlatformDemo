<?xml version="1.0" encoding="UTF-8"?>
<BackpackRoom id="backpack" xmlns:xi="http://www.w3.org/2001/XInclude">
	<Backpack>
		<Collectable id="apple" name="Apple" description="A shiny red apple, looks tasty! " previewURL="images/backpack/apple.png" quantity="1"/>
		<Collectable id="grog" name="Grog" description="Ahh, the drink of pirates " previewURL="images/backpack/grog.png" quantity="0"/>
		<Collectable id="rainwater" name="Rainwater" description="A vessel of rainwater " previewURL="images/backpack/rainwater.png" quantity="1"/>
		<Collectable id="rum" name="Rum" description="Rum, the poor mans grog " previewURL="images/backpack/rum.png" quantity="1"/>
		<Triggers>
			<OnAdded>
				<Trace message="OnAddedCollectable {Event.collectable.id}"/>
			</OnAdded>
			<OnRemoved>
				<Trace message="OnRemovedCollectable {Event.collectable.id}"/>
			</OnRemoved>
			<OnSelected>
			</OnSelected>
		</Triggers>
		<AcceptCombine>
		</AcceptCombine>
		<RejectCombine>	
			<Default>
				<Trace message="Defalt reject combine {Event.collectable.id} : {Event.collectable2.id}"/>
			</Default>
		</RejectCombine>
	</Backpack>
</BackpackRoom>
