<?xml version="1.0" encoding="utf-8"?>
<MinigameSkinViewBase 
	xmlns="uk.co.dubit.platform.components.minigame.ui.*" 
	xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%">
	
	<mx:TitleWindow width="100%" height="100%" title="{ minigame.name }" showCloseButton="true" close="{ minigame.close() }">
		<mx:Canvas id="container" width="100%" height="100%"/>
	</mx:TitleWindow>
	
	<mx:Script>
		<![CDATA[
			import mx.core.UIComponent;
		
			override public function getMinigameContainer() : UIComponent
			{
				return container;
			}		
		]]>
	</mx:Script>
	
</MinigameSkinViewBase>
